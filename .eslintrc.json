{
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "plugins": ["react"],
  "rules": { "react/react-in-jsx-scope": "off" },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  }
}
